{
  "name": "ergebnis/factory-bot",
  "description": "Provides a fixture factory for doctrine/orm entities.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "doctrine",
    "orm",
    "entity",
    "fixture",
    "factory"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/factory-bot",
  "support": {
    "issues": "https://github.com/ergebnis/factory-bot/issues",
    "source": "https://github.com/ergebnis/factory-bot"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "doctrine/annotations": "^1.10.3 || ^2.0.0",
    "doctrine/collections": "^1.6.5 || ^2.0.0",
    "doctrine/dbal": "^2.12.0 || ^3.0.0",
    "doctrine/orm": "^2.8.0",
    "doctrine/persistence": "^2.1.0 || ^3.0.0",
    "ergebnis/classy": "^1.4.0",
    "fakerphp/faker": "^1.20.0"
  },
  "require-dev": {
    "ext-pdo_sqlite": "*",
    "ergebnis/composer-normalize": "^2.36.0",
    "ergebnis/license": "^2.2.0",
    "ergebnis/php-cs-fixer-config": "^5.2.0",
    "ergebnis/phpstan-rules": "^2.1.0",
    "infection/infection": "~0.27.0",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.34",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-doctrine": "^1.3.43",
    "phpstan/phpstan-phpunit": "^1.3.14",
    "phpstan/phpstan-strict-rules": "^1.5.1",
    "phpunit/phpunit": "^9.6.12",
    "psalm/plugin-phpunit": "~0.18.4",
    "ramsey/uuid": "^4.7.4",
    "rector/rector": "~0.17.4",
    "symfony/cache": "^5.3.14",
    "vimeo/psalm": "^5.15.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\FactoryBot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\FactoryBot\\Test\\": "test/",
      "Example\\": "example/src/",
      "Example\\Test\\": "example/test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "platform": {
      "php": "8.1.21"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
